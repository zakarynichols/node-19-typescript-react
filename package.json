{
  "name": "node-19-typescript-browser",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "19.0.0",
    "npm": "8.19.2"
  },
  "os": [
    "linux"
  ],
  "description": "Node v19 with ECMAScript modules in browser environment.",
  "keywords": [
    "node",
    "dom",
    "browser",
    "v19.0.0",
    "esnext",
    "module",
    "typescript",
    "watch",
    "jest"
  ],
  "bugs": "https://github.com/zakarynichols/node-19-typescript/issues",
  "license": "MIT",
  "scripts": {
    "test": "rm -rf ./tests && tsc --project ./tsconfig.test.json && node --experimental-vm-modules node_modules/jest/bin/jest.js -c=jest.config.js --watchAll ./tests",
    "watch-typescript": "rm -rf ./public/build && tsc --watch",
    "watch-dev": "node --watch build/index.js",
    "build": "rm -rf dist && tsc --project tsconfig.prod.json",
    "bundle": "rm -rf ./public && npx webpack --config webpack.config.js"
  },
  "author": "Zakary Nichols",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "error-event-listener": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
